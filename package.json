{
  "name": "bookmarq",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf build && rimraf public/build && rimraf css",
    "postinstall": "patch-package",
    "build": "run-s build:*",
    "build:sass": "npm run generate:sass -- --style=compressed",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:sass": "npm run generate:sass -- --watch --no-source-map",
    "dev:gql": "npm run generate:gql -- --watch",
    "generate:sass": "sass app/styles/:public/css",
    "generate:gql": "graphql-codegen --config ./app/.gql/codegen.ts",
    "start": "remix-serve build",
    "test": "vitest --ui",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@extractus/article-extractor": "^7.2.9",
    "@extractus/oembed-extractor": "^3.1.8",
    "@radix-ui/react-dialog": "^1.0.2",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "dayjs": "^1.11.7",
    "droopy-typesense": "^0.0.1",
    "framer-motion": "^10.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "highlight.js": "^11.7.0",
    "html-to-text": "^9.0.4",
    "isbot": "^3.6.6",
    "jsdom": "^21.1.0",
    "jsonwebtoken": "^9.0.0",
    "just-safe-get": "^4.2.0",
    "jwt-decode": "^3.1.2",
    "linkedom": "^0.14.23",
    "match-sorter": "^6.3.1",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.7",
    "probe-image-size": "^7.2.3",
    "puppeteer": "^19.8.0",
    "puppeteer-core": "^19.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-select": "^5.7.0",
    "tailwind-merge": "^1.10.0",
    "typesense": "^1.5.3",
    "xmldom": "^0.6.0",
    "zod": "^3.20.6",
    "zod-form-data": "^2.0.0",
    "zodix": "^0.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typed-document-node": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.1.2",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/html-to-text": "^9.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/prettier": "^2.7.2",
    "@types/probe-image-size": "^7.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/xmldom": "^0.1.31",
    "@vitest/ui": "^0.29.7",
    "daisyui": "^2.51.3",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.3",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "typescript": "^5.0.2"
  }
}
